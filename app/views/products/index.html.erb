<%= link_to "Login", new_session_path, class: "login-button" unless authenticated? %>

<h1><%= t ".title" %></h1>

<%= link_to "New product", new_product_path, class: "new-product" if authenticated? %>

<div id="products">
  <% @products.each do |product| %>
    <div>
      <%= link_to product.name, product %>
    </div>
  <% end %>
</div>
